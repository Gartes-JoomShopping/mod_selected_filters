<?xml version="1.0" encoding="utf-8"?><!-- Mod_selected_filters -->
<extension type="module" version="3.8" client="site" method="upgrade">
	<name>MOD_SELECTED_FILTERS</name>
	<creationDate>11.09.2020</creationDate>
	<author>Gartes</author>
	<authorEmail></authorEmail>
	<authorUrl></authorUrl>
	<copyright>A copyright</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<version>1.0</version>
	<description>MOD_SELECTED_FILTERS_XML_DESCRIPTION</description>

	<files>
		<filename module="mod_selected_filters">mod_selected_filters.php</filename>
		<folder>tmpl</folder>
		<folder>language</folder>
		<filename>mod_selected_filters.xml</filename>
	</files>

	<!-- Update servers -->
	<updateservers>
		<server type="extension" priority="1" name="mod_selected_filters">https://www.example.com/mod_selected_filters.xml</server>
	</updateservers>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="domain"
					type="url"
					label="MOD_SELECTED_FILTERS_URL_LABEL"
					description="MOD_SELECTED_FILTERS_URL_DESCRIPTION"
					hint="https://www.example.com"
					filter="url"
					validate="url"
				/>
			</fieldset>
			<fieldset
					name="advanced">
				<field
						name="moduleclass_sfx"
						type="text"
						label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
						description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
			</fieldset>
		</fields>
	</config>
</extension>
